extends layout/layout

block vars
	-title = 'Search mua nhà'
	-bodyClass = 'page-search-res page-search page-result'

block container 
	+SearLayout("Bất động sản đang bán tại North Carolina")
		.pro-list.row.grid
			- 
				var proArrThird = [
					{
						img: "pro-7.jpg",
						time: "2 ngày trước",
						type: "Nhà ở",
						price: "$1,000",
						note: [
							{
								name: "2 bed",
								icon: "ic-pro-bed.svg"
							},
							{
								name: "2 ba",
								icon: "ic-pro-shower.svg"
							},
							{
								name: "11,300 sqft"
							},
						],
						address: "471 Tam Oshanter Dr SE, Marietta, GA, 30067"
					}
				]
			each item,index in Array(5)
				if(index == 2)
					.pro-item.col.col-6
						+proBannerSecond("pro-bner-thuenha.png")
				.pro-item.col.col-6
					+proItem2(proArrThird[0])
                                       .pro-bner-dc
                                          +img('pro-bner-dc.png')
                                       .pro-bner-img
                                          +img('pro-bner-img.jpg')
                                       .pro-bner-content 
                                          h3.pro-bner-name 
                                             |Chăm sóc
                                             br
                                             |người lớn tuổi
                                          p.pro-bner-des Dịch vụ tận tình, nhân viên chu đáo
                                          .pro-btn 
                                             span.txt Khám phá ngay
                              .pro-item.col.col-6
                                 .pro-wr.second
                                    .pro-content 
                                       .pro-flex 
                                          .pro-time Nhà hàng
                                          .pro-type Nhà hàng
                                       h3.pro-name Tuyển phục vụ nhà hàng Việt Nam
                                       ul.pro-ul 
                                          li 
                                             .pro-li
                                                +img('ic-pro-1.svg')
                                                span.txt Pho Dai Viet Restaurant
                                          li 
                                             .pro-li
                                                +img('ic-pro-2.svg')
                                                span.txt 471 Tam Oshanter Dr SE, Marietta, GA, 30067
                                       .pro-flex
                                          .pro-li
                                             +img('ic-pro-3.svg')
                                             span.txt 
                                                span(style="font-size: 1.1em").c-pri.fw-6 25 - 40
                                                span /hour
                                          .pro-li
                                             +img('ic-pro-4.svg')
                                             span.txt 500 Views Views
                        +PageNumber()
                     +footer()

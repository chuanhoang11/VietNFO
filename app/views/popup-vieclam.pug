                                    option(value="") Đăng tháng này
                                    option(value="") 3 tuần trước
                                    option(value="") 1 tháng trước
                           .col.col-btn
                              +Btn("Tìm kiếm")
            .sear-block 
               .sear-row.row 
                  .col.col-7 
                     .sear-map 
                        +img("map-img.jpg")
                  .col.col-5
                     .sear-panel 
                        .head-sc.mb-10
                           +title("Việc làm tại North Carolina", "","dcor")

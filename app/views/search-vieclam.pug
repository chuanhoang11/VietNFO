extends layout/layout

block vars
   -title = 'Search Việc làm'
   -bodyClass = 'page-search-res page-search page-result'

block container 
   +SearLayout("Việc làm tại North Carolina")
      .pro-list.row.grid
         - 
            var proArrSecond = [
               {
                  name: "Tuyển phục vụ nhà hàng Việt Nam",
                  time: "59 phút trước",
                  type: "Nhà hàng",
                  name_location: "Pho Dai Viet Restaurant",
                  name_address: "471 Tam Oshanter Dr SE, Marietta, GA, 30067",
                  price: "25 - 40",
                  views: "500"
               }
            ]
         each item, index in Array(20)
            if(index == 2)
               .pro-item.col.col-6.second
                  +proBannerSecond("pro-bner-vieclam.png")  
            .pro-item.col.col-6
               +proItemSecond(proArrSecond[0])
extends layout/layout

block vars
   -title = 'Nhà ở cho thuê'
   -bodyClass = 'page-search page-result'

block container 
   section.sec-sear 
      .sear
         .sear-wrap 
            .sear-head 
               .sear-form 
                  form(action="")
                     .b-form-gr
                        .form-list.row 
                           .col.form-ip.col-search 
                              .b-ip-search
                                 span.icon
                                    +img("icon-search.svg")
                                 span.icon-remove 
                                    i.fa-light.fa-xmark.fw-7
                                 input(type="search" placeholder="Marietta")
                           .col.col-sl-md  
                              .b-ip-select
                                 .selectCus
                                    .selectCusValue
                                       input(type="text", hidden)
                                       span.icon-arrow 
                                          i.fa-light.fa-chevron-down
                                       .txt Loại bất động sản
                                    .selectCusOptions
                                       .text-head Loại bất đọng sản
                                       label.selectCusAll 
                                          input(type="checkbox")
                                          span.txt Chọn tất cả
                                       label.selectCusOption 
                                          input(type="checkbox")
                                          span.txt Nhà ở
                                       label.selectCusOption 
                                          input(type="checkbox")
                                          span.txt Thương mại
                                       .selectCusApply Áp dụng
                           .col.col-sl-md  
                              .b-ip-select
                                 .selectTwo
                                    .selectTwoValue
                                       input(type="text", hidden)
                                       span.icon-arrow 
                                          i.fa-light.fa-chevron-down
                                       .txt Phòng ngủ - tắm
                                    .selectTwoOptions
                                       .selectTwoGr
                                          p.txt Phòng ngủ
                                          .selectTwoGrFlex
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo")
                                                span.txt any
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo")
                                                span.txt 1
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo")
                                                span.txt 2
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo")
                                                span.txt 3
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo")
                                                span.txt 4
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo")
                                                span.txt 5
                                       .selectTwoGr
                                          p.txt Phòng Tắm
                                          .selectTwoGrFlex
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo2")
                                                span.txt any
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo2")
                                                span.txt 1
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo2")
                                                span.txt 2
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo2")
                                                span.txt 3
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo2")
                                                span.txt 4
                                             label.selectTwoOption 
                                                input(type="radio", name="rdo2")
                                                span.txt 5
                                       .selectTwoApply Áp dụng
                           .col.col-sl-md
                              .b-ip-select
                                 .b-range 
                                    .range-wrap
                                       .range-head 
                                          input(type="number" hidden).rangeValue
                                          span.icon-arrow 
                                             i.fa-light.fa-chevron-down
                                          span.t-txt.rangeTextValue Giá tiền
                                       .range-panel 
                                          .range
                                             .range-flex
                                                .range-item.min
                                                   span.price 
                                                      | $500
                                                .range-item
                                                   span.price 
                                                      | $500,000
                                             .range-slider
                                                .progress
                                             .range-input 
                                                input(type="range" name="price_min" step="100" min="500" max="500000" value="0").range-min
                                                input(type="range" name="price_max" step="100" min="500" max="500000" value="500000").range-max
                                             .range-item.max.fw-6
                                                   span.price 
                                                      | $500,000
                                                   span.txt /tháng

                                    rent: "$1,500",
                                    income: "$1,000,000",
                                    address: "471 Tam Oshanter Dr SE, Marietta, GA, 30067"
                                 },
                                 {
                                    img: "pro-2.jpg",
                                    time: "2 ngày trước",
                                    type: "Nhà hàng",
                                    price: "$500,000",
                                    tag: "Thương lượng",
                                    note: [
                                       "Nam Phuong Restaurant",
                                       "36 seats",
                                       "11,300 sqft"
                                    ],
                                    rent: "$1,500",
                                    income: "$1,000,000",
                                    address: "471 Tam Oshanter Dr SE, Marietta, GA, 30067"
                                 },
                                 {
                                    img: "pro-3.jpg",
                                    time: "2 ngày trước",
                                    type: "Kinh doanh khác",
                                    price: "$500,000",
                                    note: [
                                       "Xưởng sửa chữa ô tô Đại Việt",
                                       "11,300 sqft"
                                    ],
                                    rent: "$1,500",
                                    income: "$1,000,000",
                                    address: "471 Tam Oshanter Dr SE, Marietta, GA, 30067"
                                 },
                                  {
                                    img: "pro-4.jpg",
                                    time: "2 ngày trước",
                                    type: "Kinh doanh khác",
                                    price: "$500,000",
                                    note: [
                                       "Xưởng sửa chữa ô tô Đại Việt",
                                       "11,300 sqft"
                                    ],
                                    rent: "$1,500",
                                    income: "$1,000,000",
                                    address: "471 Tam Oshanter Dr SE, Marietta, GA, 30067"
                                 },
                                   {
                                    img: "pro-2.jpg",
                                    time: "2 ngày trước",
                                    type: "Nhà hàng",
                                    price: "$500,000",
                                    tag: "Thương lượng",
                                    note: [
                                       "Nam Phuong Restaurant",
                                       "36 seats",
                                       "11,300 sqft"
                                    ],
                                    rent: "$1,500",
                                    income: "$1,000,000",
                                    address: "471 Tam Oshanter Dr SE, Marietta, GA, 30067"
                                 },
                              ]
                           each item, index in proArr
                              if(index == 2)
                                 .pro-item.col.col-6
                                    .pro-bner 
                                       .pro-bner-tag Sponsored
                                       .pro-bner-dc
                                          +img('pro-bner-dc.png')
                                       .pro-bner-img
                                          +img('pro-bner-img.jpg')
                                       .pro-bner-content 
                                          h3.pro-bner-name
                                             |Chăm sóc
                                             br
                                             |người lớn tuổi
                                          p.pro-bner-des Dịch vụ tận tình, nhân viên chu đáo
                                          .pro-btn 
                                             span.txt Khám phá ngay
                              .pro-item.col.col-6
                                 .pro-wr 
                                    .pro-img 
                                       .pro-time 
                                          span.txt #{item.time}
                                       .pro-type 
                                          span.txt #{item.type}
                                       a(href="#").inner 
                                          +img(item.img)
                                    .pro-content
                                       if item.tag
                                          .pro-top 
                                             h3.pro-name #{item.price}
                                             if item.tag
                                                .pro-tag #{item.tag}
                                       else 
                                          h3.pro-name #{item.price}
                                       if item.name
                                          h3.pro-name #{item.name}
                                       if item.note.length > 0 
                                          .pro-note
                                             each note in item.note
                                                .pro-note-item 
                                                   span.txt.fw-5.c-txt-2 #{note}
                                       if item.rent || item.income
                                          .pro-note
                                             if item.rent
                                                .pro-note-item 
                                                   span.txt.fw-5 Rent: 
                                                      span.fw-6.c-txt-3 #{item.rent}
                                                      span(style="font-size: .8em") /month
                                             if item.income
                                                .pro-note-item 
                                                   span.txt.fw-5 Income: 
                                                      span.fw-6.c-txt-3 #{item.income}
                                                      span(style="font-size: .8em") /year
                                       .pro-des #{item.address}
                        +PageNumber()
                     +footer()
